{
    "name": "zareismail/nova-cascade",
    "description": "A Laravel Nova field for the  BelongsTo and the MorphTo relation.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "zareismail"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Fields\\CascadeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
